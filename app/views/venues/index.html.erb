 <div class="containerIndex">
  <div class="grid-map">
    <div class="cards">
      <div class="grid-cards">
        <% @venues.each do |venue| %>
          <div class="card" >
            <%= link_to venue_path(venue), class:'text-decoration-none' do %>
              <%= image_tag venue.photos.first, class:"card-img-top card-venue-img pictureIndex"%>
              <div class="card-body">
                <h5 class="card-title"><%= venue.name %></h5>
                <h5 class="cardLocation"><%= venue.location %></h5>
                <br>
                <%= link_to "show",venue_path(venue), class: "btn btn-danger"%>
              </div>
            </div>
          <% end %>
        <% end %>
      </div>
    </div>
    <div class=mapIndex>
      <div id="map"
          style="height: 100vh; width: 500px;"
          data-markers="<%= @markers.to_json %>"
          data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
        </div>
    </div>
  </div>
  </div>
</div>
