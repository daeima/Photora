<div class="parent-container d-flex">
    <div class="container">
        <div class="row">
            <div class="col">
                <h1 class="text-center mt-5">New Venue</h1>
                    <%= simple_form_for(@venue) do |f| %>
                      <%= f.input :name %>
                      <%= f.input :location %>
                      <%= f.input :description %>
                      <%= f.input :price %>
                      <%= f.input :photos, as: :file, input_html: { multiple: true }%>
                      <%= f.submit %>
                    <% end %>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col">
              
            </div>
        </div>
    </div>
</div>


  
