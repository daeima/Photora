<div class="parent-container d-flex">
    <div class="container">
        <div class="row">
            <div class="col p-5">
                <h1 class="text-center mt-5">New Venue</h1>
                    <%= simple_form_for(@venue) do |f| %>
                      <%= f.input :name %>
                      <%= f.input :location %>
                      <%= f.input :description %>
                      <%= f.input :price %>
                      <%= f.input :photos, as: :file, input_html: { multiple: true }%>
                      <%= f.submit %>
                    <% end %>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col">
              <img src="https://images.unsplash.com/photo-1583847268964-b28dc8f51f92?ixid=MnwxMjA3fDB8MHxzZWFyY2h8NHx8aG9tZXxlbnwwfHwwfHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60" class="img-new" alt="Girl in a jacket" width="100%">
            </div>
        </div>
    </div>
</div>


  
